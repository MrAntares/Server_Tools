function DisablePacketEncryption() {  ///////////////////////////////////////////////  // GOAL: Find the Obfuscation Codes and NULL //  //       out the Obfuscation key generated   //  ///////////////////////////////////////////////  // Step 1a - Prep code to find & replace with  //          (and also add EDX replace if needed)  var code =      " 8B 41 08"    // MOV EAX,DWORD PTR DS:[ECX+8]    + " 0F AF 41 04" // IMUL EAX,DWORD PTR DS:[ECX+4]  var repl = " B8 00 00 00 00" // MOV EAX,0  if (exe.getClientDate() <= 20130605) {    code += " 8B 51 0C";  // MOV EDX,DWORD PTR DS:[ECX+C]    repl += " BA 00 00 00 00"; // MOV EDX,0  }  else {    code += " 03 41 0C";  // ADD EAX, [ECX+C]    repl += " 90 90 90 90 90"; // NOPs  }  //Step 1b - Find the occurances - there should be 2  var offsets = exe.findCodes(code, PTYPE_HEX, false);  if (offsets.length !== 2)    return "Failed in part 1";  //Step 2 - Replace all occurances  exe.replace(offsets[0], repl, PTYPE_HEX);    exe.replace(offsets[1], repl, PTYPE_HEX);  return true;}